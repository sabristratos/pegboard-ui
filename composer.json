{
    "name": "stratos/pegboard",
    "description": "A professional Alpine.js and TypeScript UI component library for TALL stack applications",
    "keywords": [
        "laravel",
        "livewire",
        "alpine",
        "alpinejs",
        "typescript",
        "ui",
        "components",
        "tall-stack",
        "stratos",
        "pegboard"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sabristratos/pegboard-ui",
    "support": {
        "issues": "https://github.com/sabristratos/pegboard-ui/issues",
        "source": "https://github.com/sabristratos/pegboard-ui",
        "docs": "https://github.com/sabristratos/pegboard-ui#readme"
    },
    "authors": [
        {
            "name": "Mohamed Sabri Ben Chaabane",
            "email": "sabri@stratosdigital.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "blade-ui-kit/blade-icons": "^1.8",
        "blade-ui-kit/blade-heroicons": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Stratos\\Pegboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stratos\\Pegboard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stratos\\Pegboard\\PegboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
